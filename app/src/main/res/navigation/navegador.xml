<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navegador"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.dado.MainFragment"
        android:label="Main Activity"
        tools:layout="@layout/activity_main" >
        <action
            android:id="@+id/action_mainFragment_to_diceRollsFragment"
            app:destination="@id/diceRollsFragment" >
        </action>
    </fragment>

    <fragment
        android:id="@+id/diceRollsFragment"
        android:name="com.example.dado.DiceRollsFragment"
        android:label="Dice Rolls"
        tools:layout="@layout/fragment_dice_rolls" >
        <action
            android:id="@+id/action_diceRollsFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
</navigation>